{
  "name": "link-previews",
  "version": "0.1.0",
  "description": "A React component than renders beautiful, fully-customizable link previews.",
  "keywords": [
    "react",
    "component",
    "link",
    "previews"
  ],
  "homepage": "https://github.com/Hossein-i/link-previews#readme-ov-file",
  "bugs": {
    "url": "https://github.com/Hossein-i/link-previews/issues",
    "email": "sayhi@hossein-i.ir"
  },
  "license": "MIT",
  "author": {
    "name": "Hossein-i",
    "email": "sayhi@hossein-i.ir",
    "url": "https://hossein-i.ir"
  },
  "funding": [
    {
      "type": "Zarinpal",
      "url": "https://zarinp.al/668251"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hossein-i/link-previews.git"
  },
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "sideEffects": [
    "./dist/index.js",
    "./dist/cjs/index.js"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "concurrently \"npm run build:test\" \"npm run build:types\" \"npm run build:js-css\"",
    "build:test": "jest",
    "build:types": "concurrently \"npm run tsc:*\"",
    "build:js-css": "concurrently \"npm run swc:*\" \"npm run postcss\"",
    "postcss": "webpack --config webpack.styles.config.js",
    "swc-base": "cross-env NODE_ENV=production swc src/ --config-file package.swcrc --extensions .tsx,.jsx,.ts,.js",
    "swc:es6": "npm run swc-base -- -d dist -s",
    "swc:cjs": "npm run swc-base -- -d dist/cjs -s -C module.type=commonjs",
    "tsc:es6": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration -p tsconfig.dist.json && tsc-alias",
    "tsc:cjs": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration --outDir dist/cjs/ -p tsconfig.dist.json && tsc-alias",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ncu": "ncu -i --format group",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky",
    "commit": "cz"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.5.1",
    "@storybook/cli": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-webpack5": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@svgr/webpack": "^8.1.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.9",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "cz-emoji-conventional": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.2",
    "file-loader": "^6.2.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-check-updates": "^17.1.14",
    "postcss": "^8.5.1",
    "postcss-gap-properties": "^6.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-logical": "^8.0.0",
    "postcss-modules": "^6.0.1",
    "prettier": "^3.4.2",
    "remove-files-webpack-plugin": "^1.5.0",
    "storybook": "^8.5.1",
    "swc-loader": "^0.2.6",
    "swc-plugin-css-modules": "^2.0.2",
    "swc-plugin-transform-remove-imports": "^3.0.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "resolutions": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-conventional"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.15"
  }
}
